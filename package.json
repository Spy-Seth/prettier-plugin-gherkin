{
  "name": "prettier-plugin-gherkin",
  "version": "0.0.1",
  "description": "A prettier plugin to format gherkin language (aka `.feature` files)",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "/src"
  ],
  "installConfig": {
    "pnp": true
  },
  "scripts": {
    "test": "yarn run jest",
    "lint": "yarn run eslint .",
    "format:check": "yarn run prettier --check \"./**/*.{js,json,md}\"",
    "format:fix": "yarn run prettier --write \"./**/*.{js,json,md}\""
  },
  "dependencies": {
    "gherkin": "6.0.17",
    "npm-run-path": "^3.1.0",
    "prettier": "1.17.1",
    "read-json-lines-sync": "^2.0.2"
  },
  "devDependencies": {
    "@types/prettier": "1.16.4",
    "eslint": "5.16.0",
    "eslint-plugin-jest": "22.6.4",
    "jest": "24.8.0",
    "jest-watch-typeahead": "0.3.1"
  }
}
